{
    "name": "fastbolt/entity-archiver-bundle",
    "description": "Archives Doctrine entities",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen LeuÃŸink",
            "email": "slk@fastbolt.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "symfony/console": "^6.3",
        "symfony/config": "^6.3",
        "symfony/http-kernel": "^6.3",
        "doctrine/orm": "^2.16",
        "doctrine/dbal": "^3.6",
        "symfony/phpunit-bridge": "^6.3",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^7.0",
        "fig-r/psr2r-sniffer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Fastbolt\\EntityArchiverBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
